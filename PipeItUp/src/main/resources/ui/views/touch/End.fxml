<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.fhnw.ip12.pipeitup.ui.views.touch.controllers.EndController">
   <children>
      <ImageView fitHeight="480.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/END.png" />
         </image>
      </ImageView>
      <Button fx:id="end" mnemonicParsing="false" onAction="#handleEnd" prefHeight="77.0" prefWidth="110.0" styleClass="transparentButton" stylesheets="@style/Style.css" text="end" />
      <Button fx:id="repeat" layoutX="37.0" layoutY="142.0" mnemonicParsing="false" onAction="#handleRepeat" prefHeight="143.0" prefWidth="141.0" styleClass="transparentButton" stylesheets="@style/Style.css" text="repeat" />
      <Label fx:id="endResult" layoutX="180.0" layoutY="136.0" prefHeight="208.0" prefWidth="441.0" styleClass="result" stylesheets="@style/Style.css" />
      <Button fx:id="highscore" layoutX="644.0" layoutY="162.0" mnemonicParsing="false" onMouseClicked="#handleHighscore" prefHeight="97.0" prefWidth="98.0" styleClass="transparentButton" stylesheets="@style/Style.css" text="highscore" />
      <Label fx:id="kostenSum" layoutX="75.0" layoutY="296.0" prefHeight="162.0" prefWidth="630.0" styleClass="sum" stylesheets="@style/Style.css" />
   </children>
</AnchorPane>
